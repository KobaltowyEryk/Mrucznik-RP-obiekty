public OnPlayerConnect(playerid)
{

	RemoveBuildingForPlayer(playerid, 1688, 215.9375, -43.4922, 9.9531, 0.25);//exterior
	RemoveBuildingForPlayer(playerid, 1687, 215.5938, -46.7344, 9.8594, 0.25);//exterior
	
    return 1;
}
